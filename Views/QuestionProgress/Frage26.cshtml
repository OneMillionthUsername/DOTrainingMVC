@model int
@{
	ViewData["Title"] = "Frage " + Model;
}
<h2>
Zeige alle Kunden an.
</h2>
@using (Html.BeginForm("ValidateAnswers", "QuestionProgress", FormMethod.Get,new { onsubmit = "return validateForm(['select','from'])", autocomplete = "off" }))
{
<div class="form-group">
<div id="solutionText">

<!--select-->
<input type="text" name="param0" id="param0" />
<span> * </span>
<!--from-->
<input type="text" name="param1" id="param1" />
